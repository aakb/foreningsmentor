<div{{ attributes.addClass('row','partners','bg-gray') }}>
  <div class="col-12 pt-5 pb-5">
    <div class="container">
      <div class="row">
        <div class="col text-center">
          <h2 class="mb-3">{{ content.title[0] }}</h2>
          <p class="lead">{{ content.field_description[0] }}</p>
          <div class="row justify-content-center mt-5 mb-5">
            {{ content|without('title','field_description','field_btn') }}
          </div>
          <a class="btn btn-dark" href="{{ content.field_btn[0]['#url'] }}">{{ content.field_btn[0]['#title'] }}</a>
        </div>
      </div>
    </div>
  </div>
</div>
